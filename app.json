{
  "name": "inrl-bot-md",
  "description": "whatsapp md bot build with nodejs",
  "repository": "https://github.com/inrl-official/inrl-bot-md.git",
  "keywords": [ "nodejs", "bot", "whatsapp bot", "whatsapp automation", "multi device" ],
"env": {
             "SESSION_ID": {
             "description": "The session ID you Recieved",
             "required": true
          },
             "CHATBOT ":{
             "deacripition":  "to replay automatically ",
             "required": false,
             "value":"off"
          },
             "U_STATUS": {
             "description": "For updating  profileStatus by using bot",
             "required": false,
             "value":"true"
          },
             "HEROKU_API_KEY": {
             "description": "For updating bot and setting vars remotely",
             "required": true,
             "value":""
          },
             "HEROKU_APP_NAME": {
             "description": "Exact name which you gave at the top",
             "required": true,
             "value":""
          }
},

  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  }
}
